// room script file

function obj_source_AnyClick()
{
  Wait(1);
  DynamicSprite* dynspr = DynamicSprite.Create(50, 50, true);
  
  Wait(1);
  int result = wfcOverlap(dynspr.Graphic, obj_source.Graphic, 3, true, true, 8, 0, 3);
  obj_destination.Graphic = dynspr.Graphic;
  Wait(1);
  Display("Done! %d", result);
  
  Wait(60);
}
